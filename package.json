{
  "name": "mui-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cypress/webpack-preprocessor": "^5.15.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.9",
    "@mui/lab": "^5.0.0-alpha.104",
    "@mui/material": "^5.10.10",
    "@tanstack/eslint-plugin-query": "^4.14.4",
    "@tanstack/react-query": "^4.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.2.0",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "allure-commandline": "^2.18.1",
    "cosmiconfig": "^7.0.1",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "is-ci": "^3.0.1",
    "is-ci-cli": "^2.2.0",
    "jest-watch-typeahead": "^2.2.0",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "resolve": "^1.22.1",
    "ts-loader": "latest",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.0",
    "zod": "^3.19.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage",
    "eject": "react-scripts eject",
    "jest": "jest --verbose --env=jsdom",
    "cy:test": "npm run cy:run & npm run cucumber-report",
    "cypress:runner": "cypress open",
    "cypress:execution": "cypress run --e2e --component",
    "cy:run": "cypress run --env TAGS=\"not @skip\" --browser chrome --headed",
    "cy:run:features": "cypress run --spec /cypress/**/*.features ",
    "cucumber-report": "node cucumber-html-report.js",
    "cypress:execution-sucess": "cypress run --env tags=@sucess",
    "cypress:execution-fail": "cypress run --env tags=@fail",
    "cypress:execution-allure": "cypress run  --env allure=true",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:clear2": "(if exist allure-results (rmdir /S /Q allure-results && mkdir allure-results))",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "cypress:run:loginModule": "npx cypress run --browser chrome --headed **--env allure=true --reporter mocha-allure-reporter",
    "report:login:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "test-loginModule:allure": "npm run cypress:run:loginModule && npm run report:login:allure",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@badeball/cypress-cucumber-preprocessor": "latest",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cucumber/cucumber": "^8.7.0",
    "@cypress/webpack-dev-server": "latest",
    "@shelex/cypress-allure-plugin": "^2.33.1",
    "@testing-library/cypress": "^8.0.3",
    "@types/cypress-cucumber-preprocessor": "^4.0.1",
    "cypress": "^10.10.0",
    "esbuild": "^0.15.12",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "jest-cucumber": "^3.0.1",
    "jest-environment-jsdom": "^29.2.1",
    "ts-jest": "^29.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
